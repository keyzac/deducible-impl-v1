    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.10.0",
    "husky": "4.2.5",
    "jest": "27.0.4",
    "jest-cucumber": "3.0.1",
    "serverless": "~2.69.0",
    "serverless-offline": "^5.12.0"
  },
  "dependencies": {
    "@rimac/core": "1.8.2",
    "@rimac/common": "1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
